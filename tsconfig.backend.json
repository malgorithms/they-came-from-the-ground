{
  "compilerOptions": {
    "strict": true,
    "outDir": "./backend/compiled-backend",
    "rootDir": "./backend/src-backend",
    "sourceMap": false,
    "module": "Node16",
    "target": "Es2022",
    "allowJs": false,
    "moduleResolution": "node",
    "noErrorTruncation": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmit": false,
    "allowSyntheticDefaultImports": true
  }
}
